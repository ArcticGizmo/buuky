{
  "name": "buuky",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run copy:sql:wasm && vite",
    "build:web": "npm run copy:sql:wasm && npm run build",
    "build:native": "npm run remove:sql:wasm && npm run build",
    "build": "vue-tsc && vite build",
    "copy:sql:wasm": "copyfiles -u 3 node_modules/sql.js/dist/sql-wasm.wasm public/assets",
    "remove:sql:wasm": "rimraf public/assets/sql-wasm.wasm",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@capacitor-community/sqlite": "^5.5.1-4",
    "@capacitor/app": "5.0.6",
    "@capacitor/core": "5.6.0",
    "@capacitor/haptics": "5.0.6",
    "@capacitor/keyboard": "5.0.7",
    "@capacitor/status-bar": "5.0.6",
    "@capacitor/toast": "^5.0.6",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@tanstack/vue-query": "^5.17.13",
    "ionicons": "^7.0.0",
    "vee-validate": "^4.12.4",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0",
    "vue-virtual-scroller": "^2.0.0-beta.8",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@capacitor/cli": "5.6.0",
    "@vitejs/plugin-legacy": "^5.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.3.0",
    "copyfiles": "^2.4.1",
    "cypress": "^13.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^22.1.0",
    "terser": "^5.4.0",
    "typescript": "^5.1.6",
    "vite": "^5.0.0",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.0.24"
  },
  "description": "Keep track of your reading journey today!"
}
